{
    "manifest_version": 2,

    "name": "Cache",
    "description": "For Cache Extension",
    "version": "1.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab"
    ],

    "content_scripts": [{
        "matches": ["http://*/*"],
        "js": ["jquery-3.2.1.min.js", "clear.js"],
        "css": ["clear.css"]
    }],

    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}